0] for item in samples]\n","    captions  = [item[1] for item in samples]\n","\n","    sequences = tokenizer.texts_to_sequences(captions)\n","    sequences = pad_sequences(\n","        sequences,\n","        maxlen=max_length,\n","        padding=\"post\",\n","        truncating=\"post\",\n","        value=0\n","    )\n","\n","    caption_in  = sequences[:, :-1]\n","    caption_out = sequences[:, 1:]\n","\n","    def _map(image_path, cap_in, cap_out):\n","        image = load_and_preprocess(image_path)\n","        image = tf.expand_dims(image, 0)\n","        feature = cnn_encoder(image)\n","        feature = tf.keras.layers.GlobalAveragePooling2D()(feature)\n","        feature = tf.squeeze(feature, axis=0)\n","        return (feature, cap_in), cap_out\n","\n","    ds = tf.data.Dataset.from_tensor_slices((img_paths, caption_in, caption_out))\n","    if shuffle:\n","        ds = ds.shuffle(buffer_size=min(len(img_paths), 10000))\n","    ds = ds.map(_map, num_parallel_calls=tf.data.AUTOTUNE)\n","    ds = ds.batch(batch_size).prefetch(tf.data.AUTOTUNE)\n","    return ds"]},{"cell_type":"code","execution_count":17,"id":"4b828612","metadata":{"execution":{"iopub.execute_input":"2026-02-09T20:51:05.733131Z","iopub.status.busy":"2026-02-09T20:51:05.732786Z","iopub.status.idle":"2026-02-09T20:51:06.215707Z","shell.execute_reply":"2026-02-09T20:51:06.215027Z"},"papermill":{"duration":0.499037,"end_time":"2026-02-09T20:51:06.217704","exception":false,"start_time":"2026-02-09T20:51:05.718667","status":"completed"},"tags":[]},"outputs":[],"source":["train_sample_p = preprocess_captions(train_sample)\n","test_sample_p = preprocess_captions(test_sample)\n","val_sample_p = preprocess_captions(val_sample)"]},{"cell_type":"code","execution_count":18,"id":"0241be52","metadata":{"execution":{"iopub.execute_input":"2026-02-09T20:51:06.248494Z","iopub.status.busy":"2026-02-09T20:51:06.247915Z","iopub.status.idle":"2026-02-09T20:51:06.276578Z","shell.execute_reply":"2026-02-09T20:51:06.27