nn = train_and_evaluate(\n","    rnn_model, train_ds, val_ds, test_ds, epochs=2, model_name=\"rnn\"\n",")\n","\n","# LSTM\n","lstm_model = build_lstm_model(vocab_size, seq_len, image_feature_dim)\n","hist_lstm, val_lstm, test_lstm = train_and_evaluate(\n","    lstm_model, train_ds, val_ds, test_ds, epochs=2, model_name=\"lstm\"\n",")\n","\n","# Transformer\n","tr_model = build_transformer_model(vocab_size, seq_len, image_feature_dim)\n","hist_tr, val_tr, test_tr = train_and_evaluate(\n","    tr_model, train_ds, val_ds, test_ds, epochs=2, model_name=\"transformer\"\n",")"]},{"cell_type":"code","execution_count":27,"id":"8360bca2","metadata":{"execution":{"iopub.execute_input":"2026-02-10T00:31:59.333805Z","iopub.status.busy":"2026-02-10T00:31:59.332891Z","iopub.status.idle":"2026-02-10T00:31:59.66087Z","shell.execute_reply":"2026-02-10T00:31:59.660032Z"},"papermill":{"duration":0.728331,"end_time":"2026-02-10T00:31:59.663395","exception":false,"start_time":"2026-02-10T00:31:58.935064","status":"completed"},"tags":[]},"outputs":[{"data":{"text/plain":["Text(0.5, 1.0, 'Model Accuracy Comparison')"]},"execution_count":27,"metadata":{},"output_type":"execute_result"},{"data":{"image/png":"\n","text/plain":["<Figure size 600x400 with 1 Axes>"]},"metadata":{},"output_type":"display_data"}],"source":["import pandas as pd\n","\n","results = [\n","    {\n","        \"model\": \"RNN\",\n","        \"val_loss\": val_rnn[\"loss\"],\n","        \"val_acc\": val_rnn[\"masked_accuracy\"],\n","        \"test_loss\": test_rnn[\"loss\"],\n","        \"test_acc\": test_rnn[\"masked_accuracy\"],\n","    },\n","    {\n","        \"model\": \"LSTM\",\n","        \"val_loss\": val_lstm[\"loss\"],\n","        \"val_acc\": val_lstm[\"masked_accuracy\"],\n","        \"test_loss\": test_lstm[\"loss\"],\n","        \"test_acc\": test_lstm[\"masked_accuracy\"],\n","    },\n","    {\n","        \"model\": \"Transformer\",\n","        \"val_loss\": val_tr[\"loss\"],\n","        \"val_acc\": val_tr